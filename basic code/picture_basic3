Py_pic = "C:/资料/12345/beautiful/python/1.jpg"
from PIL import Image, ImageFilter, ImageEnhance
import numpy as np
import random
img = Image.open(Py_pic)
print(img.size)
print(img.mode)
# img = img.convert("1")
# img = img.convert("L")
# img = img.convert("L")
# img = img.convert("P")
img = img.convert("RGBA")
img.show()
